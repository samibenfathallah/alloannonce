                                        <div class="col-md-8 col-sm-8 col-xs-12">
                                            <input type="date"  name="dt_effet" required="required" class="form-control col-md-7 col-xs-12"> 
                                        </div>
                                    </div>






                                    <!-- <div class="ln_solid"></div>-->

