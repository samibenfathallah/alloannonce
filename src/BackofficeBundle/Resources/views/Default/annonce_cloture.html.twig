{% block contenu %}
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
                Gestion des annonces
                <a href="{{ path('ajout_annonce') }}" class="btn btn-danger">Retourner à la liste des types annonces</a>
                <small></small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                <li class="active">Gestion des annonces</li>
            </ol>
        </section>

        <!-- Main content -->
        <section class="content">
            <!-- Small boxes (Stat box) -->
            <h1>Annonce légale clôture</h1>
            <div class="row">

                <div class="box">
                    <div class="box-header">
                        <img style="width:90%" class="box-title" src='{{ asset ('bundles/backoffice/images/2etape.png')}}'/>   
                    </div>
                    <!-- /.box-header -->

                    <div class="box-body">

                        <div class="box-body">

                            <div class="modal-body">
                                <form id="add-form"  method='POST' action='{{path('add_annonce_cloture')}}' data-parsley-validate class="form-horizontal form-label-left">

                                    <div class="form-group col-xs-12" style="background:#009ee0;color:#fff;padding-bottom:5px" >

                                        <label class="control-label  col-xs-12" style="font-size:20px;text-align: left" for="modal-name"> Siège social   </label>

                                    </div>

                                    <div class="form-group col-xs-6">

                                        <label class="control-label col-md-4 col-sm-3 col-xs-12" for="modal-name"> Adresse  <span class="required">*</span>
                                        </label>
                                        <div class="col-md-8 col-sm-8 col-xs-12">
                                            <input type="text"  name="adresse" required="required" class="form-control col-md-7 col-xs-12">
                                        </div>
                                    </div>
                                    <div class="form-group col-xs-6">
                                        <label class="control-label col-md-4 col-sm-3 col-xs-12" for="modal-name"> Complément d'adresse
                                        </label>
                                        <div class="col-md-8 col-sm-8 col-xs-12">
                                            <input type="text"  name="com_adr"  class="form-control col-md-7 col-xs-12">
                                        </div>
                                    </div>
                                    <div class="form-group col-xs-6">
                                        <label class="control-label col-md-4 col-sm-3 col-xs-12" for="modal-name"> Code Postal  <span class="required">*</span>
                                        </label>
                                        <div class="col-md-8 col-sm-8 col-xs-12">
                                            <input type="text"  name="code" required="required" class="form-control col-md-7 col-xs-12">
                                        </div>
                                    </div>

                                    <div class="form-group col-xs-6">
                                        <label class="control-label col-md-4 col-sm-3 col-xs-12" for="modal-name"> Ville  <span class="required">*</span>
                                        </label>
                                        <div class="col-md-8 col-sm-8 col-xs-12">
                                            <input type="text"  name="ville" required="required" class="form-control col-md-7 col-xs-12"> 
                                        </div>
                                    </div>


                                    <div class="form-group col-xs-6">
                                        <label class="control-label col-md-4 col-sm-3 col-xs-12" for="modal-name"> Ville du Greffe  <span class="required">*</span>
                                        </label>
                                        <div class="col-md-8 col-sm-8 col-xs-12">
                                            <input type="text"  name="greffe" required="required" class="form-control col-md-7 col-xs-12">
                                        </div>
                                    </div>


                                    <div class="form-group col-xs-12" style="background:#009ee0;color:#fff;padding-bottom:5px" >

                                        <label class="control-label col-xs-12" style="font-size:20px;text-align: left" for="modal-name"> Informations de la société  </label>

                                    </div>

                                    <div class="form-group col-xs-6">

                                        <label class="control-label col-md-4 col-sm-3 col-xs-12" for="modal-name"> Nom de la société  <span class="required">*</span>
                                        </label>
                                        <div class="col-md-8 col-sm-8 col-xs-12">
                                            <input type="text"  name="denomination" required="required" class="form-control col-md-7 col-xs-12">
                                        </div>
                                    </div>
                                    <div class="form-group col-xs-6">
                                        <label class="control-label col-md-4 col-sm-3 col-xs-12" for="modal-name"> Forme juridique de la société <span class="required">*</span>
                                        </label>
                                        <div class="col-md-8 col-sm-8 col-xs-12">

                                            <select name="forme" class="form-control col-md-7 col-xs-12">
