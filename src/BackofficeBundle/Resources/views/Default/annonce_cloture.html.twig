                                                <option value="SARL">SARL</option>
                                                <option value="SARL à capital variable">SARL à capital variable</option>
                                                <option value="SELARL">SELARL</option>
                                                <option value="SELARL à capital variable">SELARL à capital variable</option>
                                                <option value="SAS">SAS</option>
                                                <option value="SAS à capital variable">SAS à capital variable</option>
                                                <option value="SASU">SASU</option>
                                                <option value="SASU à capital variable">SASU à capital variable</option>
                                                <option value="SA">SA</option>
                                                <option value="SCI">SCI</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group col-xs-6">
                                        <label class="control-label col-md-4 col-sm-3 col-xs-12" for="modal-name"> Capital  <span class="required">*</span>
                                        </label>
                                        <div class="col-md-8 col-sm-8 col-xs-12">
                                            <input type="text"  name="capital" required="required" class="form-control col-md-7 col-xs-12">
                                        </div>
                                    </div>

                                    <div class="form-group col-xs-6">
                                        <label class="control-label col-md-4 col-sm-3 col-xs-12" for="modal-name"> N° RCS  <span class="required">*</span>
                                        </label>
                                        <div class="col-md-8 col-sm-8 col-xs-12">
                                            <input type="text"  name="rcs" required="required" class="form-control col-md-7 col-xs-12"> 
                                        </div>
                                    </div>



                                    <div class="form-group col-xs-12" style="background:#009ee0;color:#fff;padding-bottom:5px" >

                                        <label class="control-label col-xs-12" style="font-size:20px;text-align: left" for="modal-name"> Assemblée de clôture  </label>

                                    </div>

                                    <div class="form-group col-xs-6">

                                        <label class="control-label col-md-4 col-sm-3 col-xs-12" for="modal-name"> Type d'assemblée  <span class="required">*</span>
                                        </label>
                                        <div class="col-md-8 col-sm-8 col-xs-12">
                                            <select name="type_ass" class="form-control col-md-7 col-xs-12">
                                                <option value="">- Sélectionner une valeur -</option>
                                                <option value="AGO">AGO</option>
                                                <option value="AGE">AGE</option>
                                                <option value="AGM">AGM</option>
                                                <option value="Gérant">Gérant</option>
                                                <option value="Président">Président</option>
                                                <option value="Associé unique">Associé unique</option>
                                                <option value="Conseil d'administration">Conseil d'administration</option>
                                                <option value="Autre">Autre</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group col-xs-6">
                                        <label class="control-label col-md-4 col-sm-3 col-xs-12" for="modal-name"> Autre type d'assemblée 
                                        </label>
                                        <div class="col-md-8 col-sm-8 col-xs-12">
                                            <input type="text"  name="autre_type"  class="form-control col-md-7 col-xs-12">
                                        </div>
                                    </div>
                                    <div class="form-group col-xs-6">
